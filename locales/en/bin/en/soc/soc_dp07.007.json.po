#: [0]
msgid ""
"Quarter-Pounder Cheese\n"
"■This shows the\n"
"fully assembled\n"
"sandwich."
msgstr ""

#: [1]
msgid ""
"Procedure\n"
"■Put toasted quarter-pounder\n"
"buns in the special box."
msgstr ""

#: [2]
msgid ""
"Procedure\n"
"■Put the crown\n"
"on its toasted\n"
"side and the\n"
"heel next to\n"
"it cut-side-up."
msgstr ""

#: [3]
msgid ""
"Procedure\n"
"■Hit the crown\n"
"with a shot of\n"
"mustard."
msgstr ""

#: [4]
msgid ""
"Procedure\n"
"■Hit it with a\n"
"15mL shot of\n"
"ketchup."
msgstr ""

#: [5]
msgid ""
"Procedure\n"
"■Put 7g/0.2oz\n"
"of onions on\n"
"the ketchup."
msgstr ""

#: [6]
msgid ""
"Procedure\n"
"■Put 2 pickles\n"
"on top of the\n"
"onions."
msgstr ""

#: [7]
msgid ""
"Procedure\n"
"■Put one slice of\n"
"cheese on the\n"
"heel and one on\n"
"the crown."
msgstr ""

#: [8]
msgid ""
"Procedure\n"
"■Put a 1/4th\n"
"patty on top of\n"
"the cheese on\n"
"the crown side."
msgstr ""

#: [9]
msgid ""
"Procedure\n"
"■Put the heel\n"
"on top of the\n"
"assembled\n"
"crown."
msgstr ""

#: [10]
msgid ""
"Procedure\n"
"■Close the heel side of the\n"
"box on the assembled burger."
msgstr ""

#: [11]
msgid ""
"Procedure\n"
"■Turn the box upside down and\n"
"slide it down the HLZ gently."
msgstr ""
